<script type="text/javascript">
    $(function () {
        let paths = window.location.pathname.split('/');
        let brands = document.querySelector('body').getAttribute('data-all-brands').toLowerCase().split(',');
        const endpoint = '/' + paths[1] + (brands.includes(paths[2]) ? ('/' + paths[2]) : '') +'/MicroService/ProductPriceInfo/';
        fetch(endpoint, {
            method: 'post',
            body: JSON.stringify({ partNumber: "NX.VNFEB.007" })
        }).then(resp => resp.json())
          .then(res => {
                if (!res.Success) {
                    res.Price = "";
                    res.WasPrice = "";
                    res.Stock = "";
                    res.Magento = "none";
                    res.Hatch = "";
                }
                if (res.Price != '') {
                    $("#pricediv.card__price h3").text(formatCurrency("es-es", res.Price));
                }
                if (res.WasPrice != '') {
                    $("#pricediv.card__price span").text(res.WasPrice);
                }
                if (res.Magento == 'both' || res.Magento == 'magento') {
                    $("#addToCartForm").show();
                    $("#addToCartForm input.card-product__number").attr('max', res.Stock);
                    $("#addToCartForm input.card-product__number").attr('data-max-add', res.MaxQty);
                    $("#addToCartFormValidMessage").text(' Solo quedan {0} art&#237;culos en stock.'.replace(/{(\d+)}/g, res.Stock));
                    $("#whereToBuyHatch").hide();
                    $("#whereToBuyOther").hide();
                } else if (res.Magento == 'hatch') {
                    $("#addToCartForm").hide();
                    $("#whereToBuyHatch").show();
                    $("#whereToBuyHatch button").attr('data-hatch', res.Hatch);
                    $("#whereToBuyOther").hide();
                } else {
                    if (res.Magento != 'none') {
                        $("#addToCartForm").hide();
                        $("#whereToBuyHatch").hide();
                        $("#whereToBuyOther").show();
                        $("#whereToBuyOther button").attr('data-hatch', res.Hatch);
                    }
                }
          });
    })

</script>
